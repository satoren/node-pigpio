{
  "name": "node-pigpio-if",
  "version": "0.2.0",
  "description": "pigpiod client for node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepare": "rm -Rf ./dist && tsc",
    "lint": "eslint \"src/**.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\"",
    "coverage": "jest --coverage"
  },
  "keywords": [
    "raspberrypi",
    "pigpio"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/jsdom": "^16.2.4",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.7",
    "jest": "^26.6.1",
    "jsdom": "^16.4.0",
    "ts-jest": "^26.4.2",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  },
  "files": [
    "dist"
  ],
  "typedocOptions": {
    "inputFiles": [
      "./src"
    ],
    "mode": "modules",
    "out": "docs"
  },
  "exports": {
    ".": "./dist/index.js",
    "./devices": "./dist/index.js",
    "./lowlevel": "./dist/lowlevel/pi.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/satoren/node-pigpio"
  }
}
